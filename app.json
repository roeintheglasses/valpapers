{
  "expo": {
    "name": "valpapers",
    "slug": "valpapers",
    "version": "2.0.0",
    "orientation": "portrait",
    "scheme": "valpapers",
    "icon": "./assets/icons/android/res/mipmap-xhdpi/ic_launcher.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#cc3f3f"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/res/mipmap-xhdpi/ic_launcher_foreground.png",
        "backgroundImage": "./assets/icons/android/res/mipmap-xhdpi/ic_launcher_background.png",
        "monochromeImage": "./assets/icons/android/res/mipmap-xhdpi/ic_launcher_monochrome.png"
      }
    },
    "web": {
      "favicon": "./assets/icons/web/favicon.ico"
    },
    "plugins": ["expo-router", "expo-font"]
  }
}
